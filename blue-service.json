{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "bluegreenlb",
    "labels": {
      "app": "bluegreenlb"
    }
  },
  "spec": {
    "ports": [
      {
        "port": 8080,
        "targetPort": 443
      }
    ],
    "selector": {
      "app": "blue"
    },
    "type": "LoadBalancer"
  }
}
